{
  "name": "core",
  "version": "1.0.0",
  "description": "Main backend application with all logic to manage subscription operations",
  "main": "./build/main.js",
  "type": "commonjs",
  "scripts": {
    "start:dev": "npx dotenv -e .env -- npx ts-node -r ./src/open_telemetry.ts -r tsconfig-paths/register ./src/main.ts",
    "test:unit": "jest unit --passWithNoTests --watch --silent=false --config=jest.unit.config.js",
    "test:e2e": "npx dotenv -e .env.test -- npx jest e2e --detectOpenHandles --passWithNoTests --watch --silent=false --config=jest.e2e.config.js",
    "test:cov": "npx dotenv -e .env.test -- npx jest --coverage --detectOpenHandles --passWithNoTests --silent=false --config=jest.coverage.config.js",
    "test:arch": "npx jest arch --passWithNoTests --silent=false",
    "db:migrate": "node-pg-migrate --migration-file-language ts --migrations-dir ./src/migrations --envPath .env --tsconfig ./tsconfig.json --template-file-name ./migration-template.ts"
  },
  "keywords": [
    "backend",
    "typescript",
    "api",
    "modular_monolith"
  ],
  "author": "hdev14",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@faker-js/faker": "^8.4.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/i18n": "^0.13.12",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.14.2",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.15",
    "@types/pg": "^8.11.6",
    "@types/supertest": "^6.0.2",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.13.0",
    "express-validator": "^7.1.0",
    "globals": "^15.11.0",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.7",
    "node-pg-migrate": "^7.5.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "tsarch": "^5.4.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.12.2"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-metrics-otlp-proto": "^0.54.2",
    "@opentelemetry/exporter-prometheus": "^0.54.2",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.54.2",
    "@opentelemetry/instrumentation-express": "^0.44.0",
    "@opentelemetry/instrumentation-http": "^0.54.2",
    "@opentelemetry/sdk-metrics": "^1.27.0",
    "@opentelemetry/sdk-node": "^0.54.2",
    "@opentelemetry/sdk-trace-node": "^1.27.0",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.26.1",
    "cookie-parser": "^1.4.6",
    "emailjs": "^4.0.3",
    "express": "^4.19.2",
    "i18n": "^0.15.1",
    "inversify": "^6.0.2",
    "inversify-express-utils": "^6.4.6",
    "jsonwebtoken": "^9.0.2",
    "minio": "^8.0.1",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.14",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "winston": "^3.14.2"
  }
}